{
  "name": "termina",
  "version": "0.1.7-a4",
  "description": "Start process from a Webpage",
  "bin": {
    "termina": "./server.mjs"
  },
  "scripts": {
    "watch-server": "cr --emit-js",
    "compile-server": "cr --emit-js --once",
    "compile-page": "cr --emit-js --once --emit-path out-page --entry page",
    "release-page": "vite build --base=./",
    "watch-page": "cr --emit-js --emit-path out-page/ --entry page"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Termina/termina.git"
  },
  "author": "jiyinyiyong",
  "license": "MIT",
  "dependencies": {
    "@calcit/procs": "^0.6.24",
    "chalk": "^5.2.0",
    "dayjs": "^1.11.7",
    "latest-version": "^7.0.0",
    "md5": "^2.3.0",
    "nanoid": "^4.0.1",
    "randomcolor": "^0.6.2",
    "shortid": "^2.2.16",
    "url-parse": "^1.5.10",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "bottom-tip": "^0.1.4",
    "feather-icons": "^4.29.0",
    "vite": "^4.1.4"
  }
}
