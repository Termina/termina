{
  "name": "termina",
  "version": "0.1.19",
  "description": "Start process from a Webpage",
  "bin": {
    "termina": "./server.mjs"
  },
  "scripts": {
    "watch-server": "cr --emit-js",
    "compile-server": "cr --emit-js --once",
    "compile-page": "cr --emit-js --once --emit-path out-page --entry page",
    "release-page": "vite build --base=./",
    "watch-page": "cr --emit-js --emit-path out-page/ --entry page"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Termina/termina.git"
  },
  "author": "jiyinyiyong",
  "license": "MIT",
  "dependencies": {
    "@calcit/procs": "^0.8.50",
    "chalk": "^5.3.0",
    "dayjs": "^1.11.10",
    "latest-version": "^9.0.0",
    "md5": "^2.3.0",
    "nanoid": "^5.0.7",
    "randomcolor": "^0.6.2",
    "url-parse": "^1.5.10",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "bottom-tip": "^0.1.5",
    "feather-icons": "^4.29.1",
    "vite": "^5.2.10"
  }
}
