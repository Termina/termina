{
  "name": "termina",
  "version": "0.1.7-a3",
  "description": "Start process from a Webpage",
  "bin": {
    "termina": "dist/server.js"
  },
  "scripts": {
    "watch-server": "cr --emit-js",
    "build-server": "yarn webpack && node js-out/bundle.js",
    "dev-server": "hot=true webpack --watch",
    "compile-server": "cr --emit-js --once",
    "compile-page": "cr --emit-js --once --init-fn app.client/main! --reload-fn app.client/reload! --emit-path out-page",
    "release-page": "vite build --base=./",
    "release-server": "webpack",
    "watch-page": "cr --emit-js --emit-path out-page/ --init-fn app.client/main! --reload-fn app.client/reload!"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mvc-works/termina.git"
  },
  "author": "jiyinyiyong",
  "license": "MIT",
  "dependencies": {
    "@calcit/procs": "^0.5.17",
    "chalk": "^4.1.2",
    "dayjs": "^1.10.7",
    "latest-version": "^5.1.0",
    "md5": "^2.3.0",
    "randomcolor": "^0.6.2",
    "shortid": "^2.2.16",
    "url-parse": "^1.5.3",
    "ws": "^8.5.0"
  },
  "devDependencies": {
    "bottom-tip": "^0.1.3",
    "feather-icons": "^4.28.0",
    "vite": "^2.8.4",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2"
  }
}
